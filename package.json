{
  "name": "agent-development-demo",
  "version": "1.0.0",
  "description": "AIエージェントの開発能力を実演するデモプロジェクト",
  "main": "index.js",
  "scripts": {
    "start": "node javascript/server/app.js",
    "dev": "nodemon javascript/server/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint javascript/**/*.js",
    "lint:fix": "eslint javascript/**/*.js --fix",
    "format": "prettier --write \"javascript/**/*.js\"",
    "build": "npm run lint && npm test"
  },
  "keywords": [
